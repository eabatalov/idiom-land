				{
					"ver": 1,
					"id": 13,
					"type": 1,
					"label": "Hello!",
					"continue": false,
					"inCondIds": [
						4
					],
					"outCondIds": [
						7
					],
					"props": {
						"id": "editor",
						"text": "Well, Foxy, where have you been?",
						"phraseType": "SPEAK",
						"phraseSize": "MEDIUM"
					}
				},
				{
					"ver": 1,
					"id": 14,
					"type": 1,
					"label": "Editor",
					"continue": false,
					"inCondIds": [
						20
					],
					"outCondIds": [
						28
					],
					"props": {
						"id": "editor",
						"text": "It was an idiom. \"Kill two birds with one stone\" means \"to complete two tasks at one time with a single action\".",
						"phraseType": "SPEAK",
						"phraseSize": "LARGE"
					}
				},
				{
					"ver": 1,
					"id": 15,
					"type": 12,
					"label": "",
					"continue": true,
